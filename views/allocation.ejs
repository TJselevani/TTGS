<!-- ************************************************ Semester 1 *************************************************************** -->
<div id=allotedsubjectstable>
  <table>
    <caption><strong>Year 1 Semester 1 Units alloted to Lecturers</strong></caption>
    <tr>
        <th width="150">Unit Code</th>
        <th width=420>Unit Title</th>
        <!-- <th width="170">Lecturer's ID</th> -->
        <th width="330">Lecturer's Name</th>
        <th>Classroom</th>
        <th>Day</th>
        <th>Period</th>
        <th width="40">Action</th>
    </tr>
    <% 
    timetableData.forEach((semester) => {
      if(semester.semester == 1){
        semester.unitss.forEach((unit) => { %>
        <tr>
            <td><%= unit.unitCode %></td>
            <td><%= unit.unitName %></td>
            <td><%= unit.unitLecturer %></td>
            <td><%= unit.classroom %></td>
            <td><%= unit.day %></td>
            <td><%= unit.period %></td>
            <td>
            <button>DEALLOT</button>
            </td>
        </tr>
        <% });
      }
    }); %>
  </table>
</div>
<br>
<br>

